local C1 = {"DHSuprise!"}
local C2 = {"AUGUST2022!"}
local C3 = {""}

repeat task.wait(.5) until game:IsLoaded() and game:GetService("Players").LocalPlayer.Character:FindFirstChild("FULLY_LOADED_CHAR")
task.wait(2) 

for i,v in next, C1 do 
    game:GetService("ReplicatedStorage").MainEvent:FireServer("EnterPromoCode", v)
end
for i,v in next, C2 do 
    game:GetService("ReplicatedStorage").MainEvent:FireServer("EnterPromoCode", v)
end
for i,v in next, C3 do 
    game:GetService("ReplicatedStorage").MainEvent:FireServer("EnterPromoCode", v)
end
